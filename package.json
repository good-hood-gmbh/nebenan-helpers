{
  "name": "nebenan-helpers",
  "description": "React ui component helpers",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-helpers",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-helpers/issues",
  "version": "1.4.0",
  "engines": {
    "node": "^8.x.x"
  },
  "scripts": {
    "clean": "rm -rf ./lib/ || exit 1",
    "build:babel": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "prepublishOnly": "npm run clean && npm run build:babel",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run build:babel && env TZ='Europe/Berlin' mocha \"test/**/*.js\"",
    "test:tofile": "npm run build:babel && mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "babel-cli": "6.26.0",
    "babel-register": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-node-env-inline": "0.3.0",
    "chalk": "2.3.1",
    "eslint": "4.16.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-config-airbnb-base": "12.1.0",
    "mocha": "5.0.0",
    "chai": "4.1.2",
    "sinon": "4.2.0",
    "proxyquire": "1.8.0"
  },
  "peerDependencies": {
    "lodash": "^4.17.5",
    "moment": "^2.20.1"
  }
}
