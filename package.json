{
  "name": "nebenan-helpers",
  "description": "React ui component helpers",
  "author": "Nebenan.de <max@nebenan.de>",
  "license": "SEE LICENSE IN LICENSE FILE",
  "readmeFilename": "README.md",
  "homepage": "https://nebenan.de/",
  "repository": "good-hood-gmbh/nebenan-helpers",
  "bugs": "https://github.com/good-hood-gmbh/nebenan-helpers/issues",
  "version": "4.3.0",
  "scripts": {
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "check": "npm test && npm run lint && echo \"\\033[0;32mReady to publish\\033[0m\"",
    "clean": "rm -rf ./lib/ || exit 1",
    "compile": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "minify": "find ./lib -name '*.js' -exec terser {} -o {} --config-file terser.json \\;",
    "build": "npm run compile && npm run minify",
    "prepublishOnly": "npm run check && npm run clean && npm run build",
    "lint": "eslint --ext .es,.js ./",
    "lint:tofile": "eslint --ext .es,.js --no-color -o ./eslint-report.log ./",
    "test": "npm run compile && TZ='GMT' mocha \"test/**/*.js\"",
    "test:tofile": "npm run compile && TZ='GMT' mocha --no-colors -R spec \"test/**/*.js\" 2>&1 | tee test-report.log"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
    "@babel/plugin-transform-strict-mode": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "babel-plugin-add-module-exports": "^1.0.4",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "date-fns": "^2.16.1",
    "eslint": "^7.9.0",
    "eslint-config-nebenan-base": "^1.6.0",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "proxyquire": "^2.1.3",
    "qs": "^6.9.4",
    "sinon": "^9.0.3",
    "terser": "^5.3.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.15",
    "date-fns": "^2.3.0",
    "qs": "^6.9.0"
  }
}
